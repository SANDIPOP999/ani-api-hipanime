{
  "version": 2,
  "builds": [
    {
      "src": "api/meta/anilist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/meta/anilist/info/:id",
      "dest": "/api/meta/anilist/index.js"
    },
    {
      "src": "/api/meta/anilist/trending",
      "dest": "/api/meta/anilist/index.js"
    }
    {
      "src": "/",
      "dest": "/pages/index.html"
    }
  ]
}
