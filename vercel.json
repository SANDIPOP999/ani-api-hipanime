{
  "version": 2,
  "builds": [
    {
      "src": "api/meta/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/meta/banner",
      "dest": "/api/meta/index.js?type=banner"
    },
    {
      "src": "/meta/trending",
      "dest": "/api/meta/index.js"
    },
    {
      "src": "/meta/upcoming",
      "dest": "/api/meta/index.js"
    },
    {
      "src": "/meta/search",
      "dest": "/api/meta/index.js?type=search"
    },
    {
      "src": "/meta/info",
      "dest": "/api/meta/index.js?type=info"
    },
    {
      "src": "/meta/spotlights",
      "dest": "/api/meta/index.js?type=spotlights"
    },
    {
      "src": "/",
      "dest": "/pages/index.html"
    }
  ]
}
